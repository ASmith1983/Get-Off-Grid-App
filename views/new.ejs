     <div class="form-group"> 
          <label for="Trail">Trail Name:</label>
             <input type="text" id="trail" name="name" class="form-control" /> 
     </div> 
     <div class="form-group"> 
          <label for="Trail">Trail location:</label>
              <input type="text" id="trail" name="location" class="form-control" /> 
    </div> 
    <div class="form-group"> 
          <label for="Trail">Trail difficulty rating(1-5):</label>
              <input type="text" id="trail" name="difficulty" class="form-control" /> 
    </div> 
    <div class="form-group"> 
          <label for="Trail">Trail length in miles:</label>
               <input type="text" id="trail" name="length" class="form-control" /> 
    </div> 
    <div class="form-group"> 
          <label for="Trail">Trail elevation Change in feet:</label>
                <input type="text" id="trail" name="elevationChange" class="form-control" /> 
    </div> 
    <div class="form-group"> 
          <label for="Trail">Trail type:</label>
                <input type="text" id="trail" name="routeType" class="form-control" /> 
